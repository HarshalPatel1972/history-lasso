{
  "manifest_version": 3,
  "name": "History Lasso",
  "version": "1.0",
  "description": "Replaces the native history page with a superior file manager interface.",
  "permissions": [
    "history",
    "tabs",
    "favicon"
  ],
  "chrome_url_overrides": {
    "history": "dashboard.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "History Lasso"
  }
}
